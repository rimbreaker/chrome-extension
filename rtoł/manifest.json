{
    "manifest_version": 2,
    "name": "rtoł",
    "version": "1.0.0",
    "description": "an extension changing all 'r's on the webpage to 'ł's",
    "icons": {
        "16": "logo16.png",
        "48": "logo48.png",
        "128": "logo128.png"
    },
    "content_scripts": [
        {
            "permissions": [
                "activeTab"
            ],
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "script.js"
            ]
        }
    ]
}